<a href="#" class="switch_thumb">Switch Thumb</a>
<ul class="display thumb_view">
<% @artworks.each do |artwork| %>
  <li>
    <div class="content_block">
      <div class="image_block">
        <%= link_to image_tag(artwork.photo(:small)), artwork %>
      </div>    
      <h2><%= artwork.name %> </h2>
    </div>
  </li>
<% end %>  
</ul>